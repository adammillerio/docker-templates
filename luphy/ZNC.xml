<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2017-12-17</Date>
	<Beta>False</Beta>
	<Name>ZNC</Name>
	<Project>https://wiki.znc.in/ZNC</Project>
	<Overview>
		ZNC IRC bouncer.
	</Overview>
	<Description>
		ZNC IRC bouncer.
	</Description>
	<Registry>https://hub.docker.com/r/aemiller/znc/</Registry>
	<GitHub>https://github.com/adammillerio/docker-znc</GitHub>
	<Repository>adammillerio/znc:latest</Repository>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>6697</HostPort>
				<ContainerPort>6697</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
			<Port>
				<HostPort>8080</HostPort>
				<ContainerPort>8080</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/ZNC</HostDir>
			<ContainerDir>/opt/znc-data</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<Icon>https://raw.githubusercontent.com/adammillerio/docker-templates/master/icons/ZNC.png</Icon>
</Containers>