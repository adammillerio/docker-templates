<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2021-01-19</Date>
	<Beta>False</Beta>
	<Name>Synapse</Name>
	<Project>https://github.com/matrix-org/synapse</Project>
	<Overview>
		Synapse Matrix reference homeserver.
	</Overview>
	<Description>
		Synapse Matrix reference homeserver.
	</Description>
	<Registry>https://hub.docker.com/r/matrixdotorg/synapse</Registry>
	<GitHub>https://github.com/matrix-org/synapse</GitHub>
	<Repository>matrixdotorg/synapse:latest</Repository>
	<Privileged>false</Privileged>
	<Icon>https://raw.githubusercontent.com/adammillerio/docker-templates/master/icons/Synapse.png</Icon>
	<BindTime>true</BindTime>
	<Environment>
		<Variable>
			<Value>99</Value>
			<Name>UID</Name>
		</Variable>
		<Variable>
			<Value>100</Value>
			<Name>GID</Name>
		</Variable>
	</Environment>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/Synapse</HostDir>
			<ContainerDir>/data</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>8008</HostPort>
				<ContainerPort>8008</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
</Containers>