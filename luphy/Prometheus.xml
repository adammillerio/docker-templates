<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2017-11-26</Date>
	<Beta>False</Beta>
	<Name>Prometheus</Name>
	<Project>https://github.com/prometheus/prometheus</Project>
	<Overview>
		Prometheus monitoring application
	</Overview>
	<Description>
		Prometheus monitoring application
	</Description>
	<Registry>https://quay.io/repository/prometheus/prometheus</Registry>
	<GitHub>https://github.com/prometheus/prometheus</GitHub>
	<Repository>quay.io/prometheus/prometheus:v2.0.0</Repository>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
		<Port>
			<HostPort>9090</HostPort>
			<ContainerPort>9090</ContainerPort>
			<Protocol>tcp</Protocol>
		</Port>
		</Publish>
	</Networking>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/Prometheus/data</HostDir>
			<ContainerDir>/prometheus</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
		<Volume>
			<HostDir>/mnt/user/appdata/Prometheus/prometheus.yml</HostDir>
			<ContainerDir>/etc/prometheus/prometheus.yml</ContainerDir>
			<Mode>ro</Mode>
		</Volume>
	</Data>
	<Icon>https://raw.githubusercontent.com/adammillerio/docker-templates/master/icons/Prometheus.png</Icon>
</Containers>