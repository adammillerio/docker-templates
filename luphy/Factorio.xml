<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2020-03-08</Date>
	<Beta>False</Beta>
	<Name>Factorio</Name>
	<Project>https://github.com/factoriotools/factorio-docker</Project>
	<Overview>
		Factorio game server.
	</Overview>
	<Description>
		Factorio game server.
	</Description>
	<Registry>https://hub.docker.com/r/factoriotools/factorio/</Registry>
	<GitHub>https://github.com/factoriotools/factorio-docker</GitHub>
	<Repository>factoriotools/factorio:0.17.79</Repository>
	<Privileged>false</Privileged>
	<Icon>https://raw.githubusercontent.com/adammillerio/docker-templates/master/icons/Factorio.png</Icon>
	<BindTime>true</BindTime>
	<Environment>
		<Variable>
			<Name>PUID</Name>
			<Value>99</Value>
		</Variable>
		<Variable>
			<Name>PGID</Name>
			<Value>100</Value>
		</Variable>
	</Environment>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/Factorio</HostDir>
			<ContainerDir>/factorio</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>34197</HostPort>
				<ContainerPort>34197</ContainerPort>
				<Protocol>udp</Protocol>
			</Port>
			<Port>
				<HostPort>27015</HostPort>
				<ContainerPort>27015</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
</Containers>